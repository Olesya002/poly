syms x;
P = [1,4,1,-6];
[r,s,K]=residue([1,0,0],P); 
disp(r);
disp(s);
disp(r(1)/(x-s(1)));
disp(r(2)/(x-s(2)));
disp(r(3)/(x-s(3)));

%

syms x;
P = [1,-1,1,-1];
[r,s,K]=residue([1,3],P); 
disp(r);
disp(s);
disp(K);
disp(r(1)/(x-s(1)));
disp(r(2)/(x-s(2)));
disp(r(3)/(x-s(3)));

%

syms x;
P = [1,0,0,0,-1];
[r,s,K]=residue([1,0,0],P); 
disp(r);
disp(s);
disp(K);
disp(r(1)/(x-s(1)));
disp(r(2)/(x-s(2)));
disp(r(3)/(x-s(3)));
disp(r(4)/(x-s(4)));  

